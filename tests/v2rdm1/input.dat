#! cc-pvdz N2 (6,6) active space Test DQG

# job description:
print '        N2 / cc-pVDZ / DQG(6,6), scf_type = CD / 1e-12, rNN = 0.5 A'

sys.path.insert(0, '../../..')
import v2rdm_casscf

molecule n2 {
0 1
n
n 1 r
}

set {
  basis cc-pvdz
  scf_type cd
  cholesky_tolerance 1e-12
  d_convergence      1e-10
  maxiter 500
  restricted_docc  [ 2, 0, 0, 0, 0, 2, 0, 0 ]
  restricted_uocc  [ 4, 1, 2, 2, 1, 4, 2, 2 ]
}
set v2rdm_casscf {
  positivity dqg
  r_convergence  1e-4
  e_convergence  1e-4
  maxiter 20000
  orbopt_frequency 200
  mu_update_frequency 200
}

activate(n2)

n2.r     = 0.5
refscf   = -103.04337420425350 # TEST
refv2rdm = -103.086382067146   # TEST

energy('v2rdm-casscf')

compare_values(refscf, get_variable("SCF TOTAL ENERGY"), 8, "SCF total energy") # TEST
compare_values(refv2rdm, get_variable("CURRENT ENERGY"), 4, "v2RDM-CASSCF total energy") # TEST

