# v2rdm-casscf test suite

default: test

test:
	@echo "\033[0;36m"
	@echo ""
	@echo "    $(tput setaf 6)Running tests..."
	@echo ""
	@echo "\033[0;0m"
	@echo "    v2rdm1:"
	@echo ""
	@cd v2rdm1; psi4 -d
	@echo ""
	@echo "    v2rdm2:"
	@echo ""
	@cd v2rdm2; psi4 -d
	@echo ""
	@echo "    v2rdm3:"
	@echo ""
	@cd v2rdm3; psi4 -d
	@echo ""
