#! cc-pvdz N2 (6,6) active space Test DQG

# job description
print '        N2 / cc-pVDZ / DQG(6,6), scf_type = DF, rNN = 3.0 A'

sys.path.insert(0, '../../..')
import v2rdm_casscf

molecule n2 {
0 1
n
n 1 r
}

set {
  basis cc-pvdz
  scf_type df
  d_convergence      1e-10
  guess sad
  maxiter 500
  restricted_docc  [ 2, 0, 0, 0, 0, 2, 0, 0 ]
  restricted_uocc  [ 4, 1, 2, 2, 1, 4, 2, 2 ]
}
set v2rdm_casscf {
  positivity dqg
  r_convergence  1e-4
  e_convergence  1e-4
  maxiter 20000
}

activate(n2)

n2.r     = 3.0
refscf   = -107.99404148727913 # TEST
refv2rdm = -108.778127535786   # TEST

energy('v2rdm-casscf')

compare_values(refscf, get_variable("SCF TOTAL ENERGY"), 8, "SCF total energy") # TEST
compare_values(refv2rdm, get_variable("CURRENT ENERGY"), 4, "v2RDM-CASSCF total energy") # TEST

